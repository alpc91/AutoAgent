<workflow>
    <name>task_target_determination_workflow</name>
    <system_input>
        <key>user_task</key>
        <description>用户提供的任务内容，需要解析并确定目标。</description>
    </system_input>
    <system_output>
        <key>determined_target</key>
        <description>根据任务解析结果确定的最终目标。</description>
    </system_output>
    <agents>
        <agent category="existing">
            <name>任务解析智能代理</name>
            <description>此代理用于解析任务内容并提取关键信息。</description>
        </agent>
        <agent category="existing">
            <name>确定目标智能代理</name>
            <description>此代理用于根据解析后的任务内容确定具体目标。</description>
        </agent>
    </agents>
    <events>
        <event>
            <name>on_start</name>
            <inputs>
                <input>
                    <key>user_task</key>
                    <description>用户提供的任务内容，需要解析并确定目标。</description>
                </input>
            </inputs>
            <outputs>
                <output>
                    <key>user_task</key>
                    <description>用户提供的任务内容，需要解析并确定目标。</description>
                    <action>
                        <type>RESULT</type>
                    </action>
                </output>
            </outputs>
        </event>
        <event>
            <name>parse_task</name>
            <inputs>
                <input>
                    <key>user_task</key>
                    <description>用户提供的任务内容，需要解析并确定目标。</description>
                </input>
            </inputs>
            <task>
                解析用户提供的任务内容。
            </task>
            <outputs>
                <output>
                    <key>parsed_task</key>
                    <description>解析后的任务内容。</description>
                    <action>
                        <type>RESULT</type>
                    </action>
                </output>
            </outputs>
            <listen>
                <event>on_start</event>
            </listen>
            <agent>
                <name>任务解析智能代理</name>
                <model>hosted_vllm/Qwen/QwQ-32B-AWQ</model>
            </agent>
        </event>
        <event>
            <name>determine_target</name>
            <inputs>
                <input>
                    <key>parsed_task</key>
                    <description>解析后的任务内容。</description>
                </input>
            </inputs>
            <task>
                根据解析后的任务内容确定目标。
            </task>
            <outputs>
                <output>
                    <key>determined_target</key>
                    <description>根据任务解析结果确定的最终目标。</description>
                    <action>
                        <type>RESULT</type>
                    </action>
                </output>
            </outputs>
            <listen>
                <event>parse_task</event>
            </listen>
            <agent>
                <name>确定目标智能代理</name>
                <model>hosted_vllm/Qwen/QwQ-32B-AWQ</model>
            </agent>
        </event>
    </events>
</workflow>